<?xml version='1.0' encoding='utf-8'?>
<widget id="com.realdeal.loveschool" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>   
  مدرسة الحب
     </name>
    <description>
 مدرسة الحب
  
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
 مدرسة الحب

    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
   <gap:plugin name="cordova-plugin-admob" source="npm" />
    <access origin="*" />
      
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/andriod/icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/icon.png" density="ldpi" />
        <icon src="res/android/icon.png" density="mdpi" />
        <icon src="res/android/icon.png" density="hdpi" />
        <icon src="res/android/icon.png" density="xhdpi" />
        <icon src="res/android/icon.png" density="xxhdpi" />
        <icon src="res/android/icon.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
